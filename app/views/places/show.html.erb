<p id="notice"><%= notice %></p>

<h1><%= @place.name %></h1>

<p><%= @place.street %>, <%= @place.zip %> <%= @place.city %></p>

<iframe
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=<%= ENV['place_map_key'] %>&q=<%= @place.name %>+<%= @place.street %>+<%= @place.city %>" allowfullscreen>
</iframe>
